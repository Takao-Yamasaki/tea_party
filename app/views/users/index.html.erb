<%= link_to "投稿画面へ", "/users/new" %>
<% @users.each do |user| %>
  <p>
    <%= user.first_name %>
    <%= user.last_name %>
    <%= user.email %>
    <%= user.image_url %>
    <%= user.password %>
    <%= link_to '削除' "/users/#{user.id}", method: :delete, data{confirm: "削除しますか？"} %>
  </p>
<% end %>